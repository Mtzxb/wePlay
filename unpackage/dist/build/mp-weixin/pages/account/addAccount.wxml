<view class="add-account"><view class="form"><form><block wx:for="{{accountFormArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="uni-form-item uni-column"><view class="title">{{item[1]}}</view><input class="uni-input" name="{{item[0]}}" placeholder="{{item[1]}}" data-event-opts="{{[['input',[['__set_model',['$0','$1','$event',[]],['accountForm',[['accountFormArr','',index,'__$n0']]]]]]]}}" value="{{accountForm[item[0]]}}" bindinput="__e"/></view></block><view class="uni-form-item uni-column"><view class="title">是否在线</view><view><switch name="isOnline" checked="{{accountForm.isOnline==1}}" data-event-opts="{{[['change',[['changeOnlie',['$event']]]]]}}" bindchange="__e"></switch></view></view><view class="uni-btn-v"><button form-type="submit" data-event-opts="{{[['tap',[['formSubmit',['$event']]]]]}}" bindtap="__e">提交</button><button type="default" data-event-opts="{{[['tap',[['backList',['$event']]]]]}}" bindtap="__e">取消</button></view></form></view></view>